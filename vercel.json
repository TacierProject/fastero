{
  "version": 2,
  "name": "fasttero-defi",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_THIRDWEB_CLIENT_ID": "91e2d2d514dd899aa83f2f028742a060",
    "REACT_APP_NFT_CONTRACT": "0x3466ce2a1e65d564d93650d8d0f350ba5a19cdb7",
    "REACT_APP_API_BASE_URL": "https://api.fasttero.com",
    "REACT_APP_NFT_METADATA_URL": "https://metadata.fasttero.com",
    "REACT_APP_MORALIS_API": "https://deep-index.moralis.io/api/v2",
    "REACT_APP_ETHEREUM_RPC": "https://mainnet.infura.io/v3/YOUR_INFURA_KEY",
    "REACT_APP_POLYGON_RPC": "https://polygon-rpc.com",
    "REACT_APP_BSC_RPC": "https://bsc-dataseed1.binance.org",
    "REACT_APP_ENABLE_TESTNET": "false",
    "REACT_APP_ENABLE_DEBUG": "false",
    "REACT_APP_TWITTER_URL": "https://twitter.com/fasttero",
    "REACT_APP_DISCORD_URL": "https://discord.gg/fasttero",
    "REACT_APP_TELEGRAM_URL": "https://t.me/fasttero"
  }
}